.bgc{margin-top:1000rpx;display:block;width:100rpx;height:100rpx;margin:0 auto}.wrapper{padding:30rpx;-webkit-animation:slideInRight;animation:slideInRight;-webkit-animation-duration:1s;animation-duration:1s;-webkit-animation-iteration-count:1;animation-iteration-count:1}.device-area{display:flex;justify-content:space-between;flex-wrap:wrap}.warn-cart{width:700rpx;height:100rpx;border-radius:15rpx;margin-top:30rpx;display:flex;flex-wrap:wrap;box-shadow:0 0 15rpx #8f8f94}.device-data{font-size:50rpx;color:#696969}.device-info{font-size:20rpx}.warn-time{margin-left:10rpx;text-align:left;color:#6d6d6d}.warn-name{text-align:left;margin-left:25rpx;font-size:50rpx;color:#6d6d6d}.warn-logo{width:70rpx;height:70rpx;margin-left:220rpx;margin-top:15rpx}.icon-delete{width:50rpx;height:50rpx;object-fit:cover}.delete-wrap{position:absolute;right:0;top:-30rpx;width:180rpx;height:171rpx;display:flex;align-items:center;justify-content:center;z-index:999}.life-index{margin-top:10rpx;background-color:#f5f5f5;-webkit-animation:slideInLeft;animation:slideInLeft;-webkit-animation-duration:1s;animation-duration:1s;-webkit-animation-iteration-count:1;animation-iteration-count:1}.life-index .title{text-align:center;height:80rpx;line-height:80rpx;box-shadow:0 0 15rpx #8f8f94;font-size:50rpx;color:#696969}.life-index .item{border-bottom:1px solid #007aff;padding:26rpx 34rpx}.life-index .item .name{font-size:36rpx}.life-index .item .advice{font-size:24rpx}